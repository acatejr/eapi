container_commands:
    01_migrate:
        command: "python manage.py migrate"
        leader_only: true
    02_collectstatic:
        command: "python manage.py collectstatic --noinput"
    03_createsu:
        command: "python manage.py csu"
        leader_only: true
option_settings:
    aws:elasticbeanstalk:container:python:
        WSGIPath: project/wsgi.py
